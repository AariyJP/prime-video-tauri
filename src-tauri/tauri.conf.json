{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "prime-video-tauri",
  "version": "0.0.1",
  "identifier": "net.aariy.prime-video-tauri",
  "build": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "browserExtensionsEnabled": true,
        "title": "Prime Video",
        "width": 1280,
        "height": 800,
        "url": "https://www.amazon.co.jp/gp/video/storefront"
      }
    ],
    "security": {
      "csp": null,
      "dangerousDisableAssetCspModification": true
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}